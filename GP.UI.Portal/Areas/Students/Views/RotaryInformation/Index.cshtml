@{
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>主题页</title>
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/MyJs/global.js"></script>
    <link href="~/Content/MyCss/global.css" rel="stylesheet" />
    <script type="text/javascript">
        //$(function () {
        //    $("#taggle").toggle(function () {
        //        $("#BoardTitle").css("display", "block");
        //        $("#direction").html("关<br />闭<br />查<br />询");
        //    },
        //    function () {
        //        $("#BoardTitle").css("display", "none");
        //        $("#direction").html("打<br />开<br />查<br />询");
        //    });
        //});
    function Load() {
        document.getElementById("loadDiv").style.display = "none"
        document.getElementById("frmright").style.display = ""
    }
    </script>

</head>
<body onload="Load()">
    <table cellspacing="0" cellpadding="0"  style="height: 100%; width:100%">
        <tr>
            <td class="listTableHeadTR" style="height: 22px;" colspan="3">
                <img height="16" src="/images/imgs/Query.gif" width="16" style="vertical-align: middle" />轮转信息查询结果
            </td>
        </tr>
        <tr>
            <td valign="top" >
                @*<iframe id="BoardTitle" style="display: none; visibility: inherit; width: 200px; height: 100%" src='@Url.Action("Left")' frameborder="0" scrolling="yes"></iframe>*@
                <iframe id="BoardTitle" style="display: none; visibility: inherit; width: 200px; height: 100%" src='' frameborder="0" scrolling="yes"></iframe>
            </td>
            <td>
                <table cellspacing="0" cellpadding="0" border="0" style="height:100%; width:16px;">
                    <tr>
                        <td id="taggle" style="text-align:center;cursor: pointer;width:16px; height: 100%;background:url(/images/imgs/main1.gif) repeat; vertical-align:top; padding-top:30px;">
                            <span style="display:none;" id="switchPoint" title="关闭/打开左栏">closed</span>
                            <span id="direction" style="color:red; font-weight:bold; font-size:14px;"></span>
                            @*<span id="direction" style="color:red; font-weight:bold; font-size:14px;">打<br />开<br />查<br />询</span>*@
                        </td>
                    </tr>
                </table>
            </td>
            <td style="height: 100%; width: 100%;" valign="top">
                <div id="loadDiv" style="display:block;position:absolute;top:50%;left:40%;"><img src="/images/Loading/8.gif" /></div>
                <iframe id="frmright" style="visibility: inherit; width: 100%; height: 100%;display:none;" name="frmright" src='@Url.Action("List")' frameborder="0" scrolling="auto"></iframe>
            </td>
        </tr>
    </table>
</body>
</html>
